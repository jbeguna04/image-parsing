<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>image-parsing</title>
  <% if (htmlWebpackPlugin.options.nodeModules) { %>
    <script>
      require('module').globalPaths.push(
        `<%= htmlWebpackPlugin.options.nodeModules.replace(/\\/g, '\\\\') %>`
      )
    </script>
    <% } %>
</head>

<body>
  <div id="app"></div>
  <!-- Set `__static` path to static files in production -->
  <script>
    if (process.env.NODE_ENV !== 'development') window.__static = require(
      'path').join(__dirname, '/static').replace(/\\/g, '\\\\')
  </script>
  <script src="./static/script/fabric.js"></script>
  <!-- webpack builds are automatically injected -->
</body>

</html>
