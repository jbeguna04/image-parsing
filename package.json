{
  "author": "Muhammad Ubaid Raza <mubaidr@gmail.com>",
  "bugs": {
    "url": "https://github.com/mubaidr/image-parsing/issues"
  },
  "build": {
    "appId": "org.mubaidr.image-parsing",
    "directories": {
      "output": "build"
    },
    "dmg": {
      "contents": [
        {
          "path": "/Applications",
          "type": "link",
          "x": 410,
          "y": 150
        },
        {
          "type": "file",
          "x": 130,
          "y": 150
        }
      ]
    },
    "files": [
      "dist/electron/**/*"
    ],
    "linux": {
      "icon": "build/icons"
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "productName": "image-parsing",
    "win": {
      "icon": "build/icons/icon.ico"
    }
  },
  "dependencies": {
    "brain.js": "^1.1.2",
    "bulma-fluent": "^0.1.33",
    "sharp": "^0.20.1",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.2"
  },
  "description": "An electron-vue project",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.2.0",
    "chai": "^4.1.2",
    "chalk": "^2.3.2",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.0-beta.7",
    "electron-builder": "^20.8.1",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.11.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.1.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^14.2.2",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.22.0",
    "webpack-merge": "^4.1.2"
  },
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "name": "image-parsing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mubaidr/image-parsing.git"
  },
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix",
    "test": "npm run unit",
    "unit": "karma start test/unit/karma.conf.js"
  },
  "version": "0.0.1"
}
